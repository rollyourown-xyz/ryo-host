default-remote: local
remotes:
  images:
    addr: https://images.linuxcontainers.org
    protocol: simplestreams
    public: true
  local:
    addr: unix://
    public: false
  {{ project_id }}:
    addr: https://{{ wireguard_address_network_part }}.2:8443
    auth_type: tls
    project: default
    protocol: lxd
    public: false
  ubuntu-minimal:
    addr: https://cloud-images.ubuntu.com/minimal/releases/
    protocol: simplestreams
    public: true
aliases: {}